doctype html
html(lang="ru").no-js
  head
    title Ресторан паназиатской кухни «Шикари»
    meta(charset="utf-8")
    meta(http-equiv="x-ua-compatible" content="ie=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no")
    meta(name="msapplication-tap-highlight" content="no")
    link(rel="stylesheet" href="css/vendor.css")
    link(rel="stylesheet" href="css/app.css")
    link(href="https://fonts.googleapis.com/css?family=Comfortaa|Noto+Sans:700&amp;subset=cyrillic" rel="stylesheet")
    script(async src="js/modernizr.js")
  body
    include _content
    include _mixins
    header(class="m_header")
      .wrapper
        .top
          //.cart
          //  a(href="#"): span 0 руб
          .socials
            a(class="vk" href="#" target="_blank")
            a(class="fb" href="#" target="_blank")
            a(class="instagram" href="#" target="_blank")
          .action
            a(href="#") Раскуси <span>Азию</span>
        .bottom
          .logo: a(href="#"): img(src="images/logo.svg" alt="")
          .btn-mobile-menu: i
          .menu
            nav
              - var nav = ["Меню", "Доставка", "Почетный гость", "Акции", "Контакты", "О нас"]
              ul
                each value in nav
                  li: a(href="#"): span(data-line=value)= value
          .contacts
            .centered
              .tel: a(href="tel:+74957810781") +7 (495) 781-0-781
              .locate: a(href="#") Ваш регион: <span>Москва</span>
    main
      section.m_intro
        .images
          img(data-parallaxify-range-x="-20" data-parallaxify-range-y="20" class="intro-16" src="images/intro/16.png" alt="")
          img(data-parallaxify-range-x="-20" data-parallaxify-range-y="20" class="intro-17" src="images/intro/17.png" alt="")
          img(data-parallaxify-range-x="20" data-parallaxify-range-y="10" class="intro-12" src="images/intro/12.png" alt="")
          img(data-parallaxify-range-x="30" data-parallaxify-range-y="45" class="intro-1" src="images/intro/1.png" alt="")
          img(data-parallaxify-range-x="50" data-parallaxify-range-y="20" class="intro-2" src="images/intro/2.png" alt="")
          img(data-parallaxify-range-x="10" data-parallaxify-range-y="5" class="intro-3" src="images/intro/3.png" alt="")
          img(data-parallaxify-range-x="-20" data-parallaxify-range-y="30" class="intro-4" src="images/intro/4.png" alt="")
