html, body
  height 100%
main
  height 100%
  > section
    height 100%
    padding-top 59px
    @media screen and (min-width: 992px) and (min-height: 900px)
      padding-top 173px
    > .wrapper
      display table
      width 100%
.wrapper
  clearfix()
  position relative
  max-width $wrapper
  height 100%
  padding-left 15px
  padding-right 15px
  margin-left auto
  margin-right auto